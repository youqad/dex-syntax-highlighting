{
  "name": "dex-syntax-highlighting",
  "displayName": "Dex Syntax Highlighting",
  "description": "Syntax highlighting support for Dex in Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "youqad",
  "engines": {
    "vscode": "^1.75.0"
  },
  "keywords": [
    "language",
    "dex",
    "highlight",
    "syntax"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/youqad/dex-syntax-highlighting",
  "repository": {
    "type": "git",
    "url": "https://github.com/youqad/dex-syntax-highlighting.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dex",
      "aliases": ["Dex", "dex"],
      "extensions": [".dx"],
      "configuration": "./dex-configuration.json"
    }],
    "grammars": [{
      "language": "dex",
      "scopeName": "source.dex",
      "path": "./syntaxes/dex.tmLanguage.json"
    }]
  }
}
